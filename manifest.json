{
  "manifest_version": 3,
  "name": "Management Helper",
  "version": "1.1.0",
  "description": "把每个需求的相关链接集中管理，支持一键收录、展开查看、Done 归档与归档管理。",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_title": "Management Helper",
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "tabs", "contextMenus"],
  "background": {
    "service_worker": "background.js"
  }
}